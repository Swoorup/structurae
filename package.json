{
  "name": "structurae",
  "version": "4.0.0-beta.1",
  "description": "Data structures for performance-sensitive modern JavaScript applications.",
  "main": "index.js",
  "type": "module",
  "keywords": [
    "optimization",
    "binary",
    "binary protocol",
    "data structures",
    "sorted",
    "array",
    "bitfield",
    "bigint",
    "graph",
    "matrix",
    "heap",
    "list",
    "adjacency"
  ],
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "doc": "typedoc --out doc src/index.ts"
  },
  "files": [
    "*.js",
    "*.d.ts"
  ],
  "author": "Maga D. Zandaqo <denelxan@gmail.com> (http://maga.name)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zandaqo/structurae.git"
  },
  "homepage": "https://github.com/zandaqo/structurae#readme",
  "bugs": {
    "url": "https://github.com/zandaqo/structurae/issues"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "benchmark": "^2.1.4",
    "jest": "^27.0.3",
    "json-schema-faker": "^0.5.0-rcv.34",
    "ts-jest": "^27.0.2",
    "typescript": "^4.3.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(js|ts)$": "ts-jest"
    },
    "testMatch": [
      "**/test/**/*.spec.ts"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/**",
      "!**/node_modules/**",
      "!**/__snapshots__/**",
      "!**/__fixtures__/**"
    ],
    "coverageDirectory": "<rootDir>/coverage",
    "coverageReporters": [
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "engines": {
    "node": ">=11.0.0"
  }
}
