{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "sourceMap": true,
    "inlineSources": true,
    "isolatedModules": true,
    "declaration": true,
    "skipLibCheck": true,
    "outDir": "./build",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "lib": [
      "es2020",
    ]
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": []
}