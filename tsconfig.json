{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "sourceMap": true,
    "inlineSources": true,
    "isolatedModules": true,
    "declaration": true,
    "skipLibCheck": true,
    "outDir": "./",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "lib": [
      "es2020"
    ]
  },
  "include": [
    "*.ts"
  ],
  "exclude": [
    "*.d.ts",
    "dev_deps.ts"
  ]
}
